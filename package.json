{
  "name": "node-labs",
  "version": "1.0.0",
  "description": "A simple online shop for various products",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "data:import": "node src/seeder",
    "data:destroy": "node src/seeder -d",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GluterusMaximus/node-labs.git"
  },
  "author": "Grigoriy Ustymenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GluterusMaximus/node-labs/issues"
  },
  "homepage": "https://github.com/GluterusMaximus/node-labs#readme",
  "devDependencies": {
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "husky": "^8.0.1",
    "prettier": "^2.6.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.3.5"
  }
}
